algoritmo "semnome"

var

           N1, N2, N3: inteiro
           cont: inteiro

inicio

          N1 <- 0
          Escreva (N1)
          N2 <- 1
          Escreva (N2)
          Para cont <- 3 ate 15 faca
             N3 <- N2+N1
             Escreva (N3)
             N1 <- N2
             N2 <- N3
          FimPara


fimalgoritmo

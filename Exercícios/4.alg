algoritmo "semnome"

var

   x1, x2, x3: real

inicio

        Escreval ("Qual é seu primeiro número?")
        Leia (x1)
        Escreval ("Qual é seu segundo número?")
        Leia (x2)
        x3 <- (x1+x2)
        Escreval ("A soma do primeiro com o segundo número é igual a:", x3)


fimalgoritmo

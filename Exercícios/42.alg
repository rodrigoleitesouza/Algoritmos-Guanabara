algoritmo "semnome"

var

        N: inteiro
        c: inteiro

inicio
                 c <- 0
                 Escreva ("Digite um número: ")
                 Leia (N)
                 
                 Enquanto (c<=N) faca
                    Escreva (c,"..")
                    c <- (c+1)
                 FimEnquanto
                 Escreva (" Acabou..")
                 

fimalgoritmo

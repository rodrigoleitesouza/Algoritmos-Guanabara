algoritmo "semnome"

var

      x1, x2, x3: real

inicio

        Escreval ("Digite um número e descubra seu dobro e sua terça parte.")
        Leia (x1)
        x2 <- (x1*2)
        x3 <- (x1/3)
        Escreval ("O dobre de", x1," é igual a", x2,".")
        Escreval ("A terça parte de", x1," é igual a", x3,".")

fimalgoritmo

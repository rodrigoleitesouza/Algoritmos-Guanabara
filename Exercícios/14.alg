algoritmo "semnome"


var

       x1, x2, x3: real

inicio

        Escreval ("Quantos KMs foram percorridos?")
        Leia (x1)
        Escreval ("Quantos dias o carro ficou alugado?")
        Leia (x2)
        x3 <- ((90*x2) + (0.2*x1))
        Escreval ("O valor total a pagar é R$",x3,".")
        

fimalgoritmo
